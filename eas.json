{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.19.2",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "preview": {
      "distribution": "internal", 
      "node": "20.19.2",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.19.2",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "credentialsSource": "remote"
    }
  },
  "submit": {
    "production": {}
  }
}