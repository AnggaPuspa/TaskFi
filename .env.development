# ================================================================
# ENVIRONMENT CONFIGURATION - DEVELOPMENT
# ================================================================

# Application Environment
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_BUILD_NUMBER=1

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OCR Configuration - REAL OCR ENABLED
EXPO_PUBLIC_OCR_MOCK_MODE=false
EXPO_PUBLIC_OCR_CONFIDENCE_THRESHOLD=0.5
EXPO_PUBLIC_OCR_PROCESSING_INTERVAL=500
EXPO_PUBLIC_OCR_MAX_RETRIES=2
EXPO_PUBLIC_OCR_TIMEOUT=5000

# Performance Configuration
EXPO_PUBLIC_MAX_CONCURRENT_OCR_TASKS=1
EXPO_PUBLIC_ENABLE_FRAME_SKIPPING=true
EXPO_PUBLIC_ENABLE_IMAGE_COMPRESSION=false
EXPO_PUBLIC_MAX_IMAGE_SIZE=1024

# Camera Configuration  
EXPO_PUBLIC_CAMERA_HIGH_QUALITY=false
EXPO_PUBLIC_CAMERA_STABILIZATION=false
EXPO_PUBLIC_CAMERA_PIXEL_FORMAT=yuv
EXPO_PUBLIC_CAMERA_COMPRESSION_QUALITY=0.8

# Feature Flags - REAL OCR ENABLED
EXPO_PUBLIC_ENABLE_REALTIME_OCR=true
EXPO_PUBLIC_ENABLE_AUTO_TRANSACTION_CREATION=true
EXPO_PUBLIC_ENABLE_RECEIPT_STORAGE=true
EXPO_PUBLIC_ENABLE_DEBUG_MODE=true

# API Configuration
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_SYNC_INTERVAL=30000

# Debugging
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_ENABLE_METRICS_DISPLAY=true

# Security
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_SESSION_TIMEOUT=3600000

# Analytics
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ANALYTICS_API_KEY=

# Push Notifications
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_NOTIFICATION_SOUND=default
